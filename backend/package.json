{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "main": "servers/api-server.js",
  "scripts": {
    "start": "./start-servers.sh",
    "dev": "./start-servers.sh",
    "start:api": "node servers/api-server.js",
    "start:docs": "node servers/documents-server.js",
    "start:proxy": "node servers/proxy-server.js",
    "dev:api": "nodemon servers/api-server.js",
    "dev:docs": "nodemon servers/documents-server.js",
    "dev:proxy": "nodemon servers/proxy-server.js",
    "seed": "node prisma/seed.js",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:reset": "npx prisma migrate reset",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" npx jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:proxy": "cd tests && npm test",
    "test:proxy:watch": "cd tests && npm run test:watch",
    "test:e2e": "cd tests && npm run test:proxy",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "npm run lint && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format"
  },
