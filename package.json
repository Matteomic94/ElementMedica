    "test:performance": "playwright test tests/performance",
    "test:regression": "playwright test tests/regression",
    "test:integration": "playwright test tests/integration",
    "test:all": "npm run test && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e -- --reporter=junit",
    "test:smoke": "playwright test tests/smoke",
    "dev:all": "concurrently \"npm run dev\" \"cd backend && npm start\"",
    "api": "concurrently \"npm run dev\" \"cd backend && npm start\"",
    "start": "concurrently \"npm run dev\" \"cd backend && npm start\"",
    "start:frontend": "vite",
    "start:backend": "cd backend && npm start",
